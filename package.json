{
  "name": "canvas-component",
  "version": "0.0.1",
  "description": "Web Components made by Stencil.js",
  "main": "dist/index.cjs.js",
  "module": "dist/components/index.js",
  "types": "dist/components/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/canvas-component/canvas-component.esm.js",
  "exports": {
    ".": {
      "import": "./dist/canvas-component/canvas-component.esm.js",
      "require": "./dist/canvas-component/canvas-component.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/canvas-component.js",
      "types": "./dist/components/canvas-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thoschu/stencil.js.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "⚠️": "npm i",
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "eslint:prettier": "npx eslint-config-prettier",
    "prettier:check": "prettier --check src",
    "prettier:write": "prettier --write --ignore-unknown src",
    "prepublish": "npm run build",
    "publish": "npm publish --access public",
    "clean": "npx concurrently 'npx rimraf dist' 'npx rimraf loader' 'npx rimraf www' 'npx rimraf .stencil' 'npx rimraf node_modules'",
    "surf": "npx http-server ./private",
    "surf:src": "npx http-server ./src",
    "surf:www": "npx http-server ./www",
    "docker:verdaccio": "docker run -it --rm --name verdaccio -p 4873:4873 -d verdaccio/verdaccio",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@stencil-community/eslint-plugin": "^0.8.0",
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.12",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "autoprefixer": "^10.4.20",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "ramda": "^0.30.1"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx,css,scss,html}": "npm run prettier:write"
  }
}
